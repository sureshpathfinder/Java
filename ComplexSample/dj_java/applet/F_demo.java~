import java.awt.*;
import java.applet.*;

/*<applet code=F_demo height=100 width=100></applet>*/
public static void main(String s[])
{
  Font f,f1;
  public void init()
  {
    f=new Font("Arial Black",Font.BOLD,25);
    f1=new Font("Times Roman",Font.ITALIC,50);
  }
  
  public void paint(Graphics g)
  {
    g.setFont(f);
    g.drawString("Arial Black",100,100);
    g.setFont(f1);
    g.drawString("Times Roman",100,100);
  }
}
    