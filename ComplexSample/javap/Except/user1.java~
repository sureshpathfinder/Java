import java.util.*;
class user 
{
  public static void main(String args[])
  {
    int i;
    Scanner s=new Scanner(System.in);
    Exception e=new Exception();
    
    i=s.nextInt();
    try
    {
      
      if(i<0) 
        throw (e);
      System.out.println("hello="+i);
    }
    catch(Exception n)
    {
      System.out.println("Error="+n);
    }
  }
}

