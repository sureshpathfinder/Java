abstract class Base
{
  int length, breadth;
  abstract int area(int a,);
  public void setArea(int length, int breadth)
  {
   this.length = length;
   this.breadth = breadth;
   
   pw.println("Calculating the area of a ..");
   pw.println("The area is  ");
   pw.println(area());
 }
}
