class Main_Final_Method
{
  public static void main(String args[])
  {
    FinalD_Method f=new FinalD_Method();
    f.show();
  }
}

class FinalB_Method
{
  final void show()
  {
    System.out.println("Final Base Method");
  }
}

class FinalD_Method extends FinalB_Method
{
  void show()
  {
    System.out.println("Final Derived Mathod");
  }
}

