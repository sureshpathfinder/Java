class staticfunc
{
  int i;
  static int j;
  public static void func()
  {
    System.out.println("you can access a static method this way");
  }
  public void disp()
  {
    i=100;
    j=1000;
    System.out.println("Don't try to access a non static method");
  }
  public static void main(String arg[])
  {
    
    //i=100;    
    j=1000;
    func();
    //disp();
  }
} 